{
  "name": "@cox/core-services-util",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build": "rimraf dist && ngc -p src/tsconfig.aot.json && gulp",
    "cleanup-umd": "rimraf dist/bundles dist/src dist/index.d.ts dist/index.js dist/index.js.map dist/LICENCE dist/README.md",
    "bundling-umd": "rollup -c rollup-config-umd.js",
    "minify-umd": "uglifyjs dist-umd/bundles/coreServicesUtil.umd.js --screw-ie8 --compress --mangle --comments --output dist-umd/bundles/coreServicesUtil.umd.min.js",
    "remove-ngc-files": "rimraf **/*.shim.ngstyle.ts && rimraf **/*.ngfactory.ts && rimraf **/*.ngsummary.json",
    "build-umd-org": "npm run cleanup-umd && ngc && npm run bundling-umd && npm run minify-umd && npm run remove-ngc-files",
    "build-umd": "npm run remove-ngc-files && npm run cleanup-umd && ngc -p src/tsconfig.json && gulp copyHtml && gulp inlineResourceUmd && npm run bundling-umd && npm run minify-umd && npm run remove-ngc-files"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "core-js": "^2.4.1",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2",

    "@types/file-saver": "0.0.0",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.2.34",
    "file-saver": "^1.3.3",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.11"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "angular-cli": "1.0.0-beta.28.3",
    "codelyzer": "~2.0.0-beta.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3",

    "gulp": "^3.9.1",
    "gulp-sass": "^2.3.2",

    "rimraf": "^2.5.4",
    "rollup": "^0.41.5",
    "rollup-plugin-commonjs": "^7.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1",
    "uglify-js": "^2.7.5",
    "uglifyjs": ""
  }
}
